{
	"$schema": "vscode://schemas/color-theme",
	"name": "MacVim Classic Light",
	"type": "light",
	"colors": {
		"editor.background": "#ffffff",
		"editor.foreground": "#000000",
		"editorCursor.foreground": "#000000",
		"editorLineNumber.foreground": "#888888",
		"editorLineNumber.activeForeground": "#000000",
		"editor.selectionBackground": "#72f7ff",
		"editor.selectionHighlightBackground": "#72f7ff80",
		"editor.lineHighlightBackground": "#f1f5fa",
		"editorIndentGuide.background1": "#e6e6e6",
		"editorIndentGuide.activeBackground1": "#d0d0d0",
		"editorWhitespace.foreground": "#e6e6e6",
		"editorWidget.background": "#ffffff",
		"editorWidget.border": "#e6e6e6",
		"editorSuggestWidget.background": "#ffffff",
		"editorSuggestWidget.border": "#e6e6e6",
		"editorSuggestWidget.foreground": "#000000",
		"editorSuggestWidget.selectedBackground": "#98f5ff",
		"editorSuggestWidget.highlightForeground": "#458b74",
		"editorError.foreground": "#cd0000",
		"editorWarning.foreground": "#ff8c00",
		"editorInfo.foreground": "#1874cd",
		"editorBracketMatch.background": "#ab82ff",
		"editorBracketMatch.border": "#ffffff",
		"editor.findMatchBackground": "#98f5ff",
		"editor.findMatchHighlightBackground": "#98f5ff80",
		"activityBar.background": "#e6e6e6",
		"activityBar.foreground": "#000000",
		"activityBarBadge.background": "#1874cd",
		"activityBarBadge.foreground": "#ffffff",
		"sideBar.background": "#e6e6e6",
		"sideBar.foreground": "#000000",
		"sideBarTitle.foreground": "#000000",
		"sideBarSectionHeader.background": "#ffffff",
		"sideBarSectionHeader.foreground": "#000000",
		"list.activeSelectionBackground": "#98f5ff",
		"list.activeSelectionForeground": "#000000",
		"list.inactiveSelectionBackground": "#f1f5fa",
		"list.hoverBackground": "#f1f5fa",
		"list.focusBackground": "#98f5ff",
		"statusBar.background": "#e6e6e6",
		"statusBar.foreground": "#000000",
		"statusBar.noFolderBackground": "#e6e6e6",
		"statusBar.debuggingBackground": "#cd0000",
		"statusBarItem.prominentBackground": "#1874cd",
		"titleBar.activeBackground": "#ffffff",
		"titleBar.activeForeground": "#000000",
		"titleBar.inactiveBackground": "#e6e6e6",
		"titleBar.inactiveForeground": "#888888",
		"tab.activeBackground": "#ffffff",
		"tab.inactiveBackground": "#e6e6e6",
		"tab.activeForeground": "#000000",
		"tab.inactiveForeground": "#888888",
		"tab.border": "#e6e6e6",
		"tab.activeBorder": "#1874cd",
		"input.background": "#ffffff",
		"input.foreground": "#000000",
		"input.border": "#e6e6e6",
		"inputOption.activeBorder": "#1874cd",
		"button.background": "#1874cd",
		"button.foreground": "#ffffff",
		"button.hoverBackground": "#1e90ff",
		"dropdown.background": "#ffffff",
		"dropdown.foreground": "#000000",
		"dropdown.border": "#e6e6e6",
		"panel.background": "#ffffff",
		"panel.border": "#e6e6e6",
		"panelTitle.activeForeground": "#000000",
		"panelTitle.inactiveForeground": "#888888",
		"badge.background": "#008b00",
		"badge.foreground": "#ffffff",
		"progressBar.background": "#1874cd",
		"scrollbarSlider.background": "#88888866",
		"scrollbarSlider.hoverBackground": "#666666b3",
		"scrollbarSlider.activeBackground": "#00000066"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#0000ee",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"string.quoted",
				"string.template"
			],
			"settings": {
				"foreground": "#4a708b"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#ff8c00"
			}
		},
		{
			"name": "Boolean",
			"scope": [
				"constant.language.boolean",
				"constant.language.null"
			],
			"settings": {
				"foreground": "#cd0000"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"storage.type.function"
			],
			"settings": {
				"foreground": "#800000",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function",
				"meta.function-call.generic",
				"support.function"
			],
			"settings": {
				"foreground": "#458b74"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
				"variable.other",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#458b74"
			}
		},
		{
			"name": "Type",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#008b00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant",
				"constant.other",
				"constant.language"
			],
			"settings": {
				"foreground": "#ff8c00"
			}
		},
		{
			"name": "Preprocessor",
			"scope": [
				"keyword.control.import",
				"keyword.control.from",
				"entity.name.function.preprocessor",
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#1874cd"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#800000"
			}
		},
		{
			"name": "Attribute",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#458b74"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator",
				"punctuation"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Special",
			"scope": [
				"variable.language",
				"support.variable"
			],
			"settings": {
				"foreground": "#8a2be2"
			}
		},
		{
			"name": "Regex",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#4a708b"
			}
		},
		{
			"name": "Todo",
			"scope": [
				"comment.todo",
				"comment.note",
				"comment.fixme",
				"comment.hack",
				"comment.xxx"
			],
			"settings": {
				"foreground": "#006400"
			}
		},
		{
			"name": "Error",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#cd0000"
			}
		},
		{
			"name": "Markdown Heading",
			"scope": [
				"markup.heading",
				"entity.name.section.markdown"
			],
			"settings": {
				"foreground": "#1874cd",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Bold",
			"scope": [
				"markup.bold.markdown"
			],
			"settings": {
				"foreground": "#458b74",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Italic",
			"scope": [
				"markup.italic.markdown"
			],
			"settings": {
				"foreground": "#4a708b",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown Code",
			"scope": [
				"markup.inline.raw.markdown",
				"markup.fenced_code.block.markdown"
			],
			"settings": {
				"foreground": "#4a708b"
			}
		},
		{
			"name": "JSON Key",
			"scope": [
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#458b74"
			}
		},
		{
			"name": "CSS Property",
			"scope": [
				"support.type.property-name.css"
			],
			"settings": {
				"foreground": "#458b74"
			}
		},
		{
			"name": "CSS Value",
			"scope": [
				"support.constant.property-value.css"
			],
			"settings": {
				"foreground": "#4a708b"
			}
		}
	]
}



