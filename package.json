{
	"name": "macvim-classic-theme",
	"displayName": "MacVim Classic",
	"description": "A VS Code theme inspired by MacVim's classic color scheme (dark and light versions)",
	"version": "1.0.0",
	"publisher": "JuanVqz",
	"license": "MIT",
	"engines": {
		"vscode": "^1.17.0"
	},
	"categories": [
		"Themes"
	],
	"keywords": [
		"theme",
		"dark theme",
		"light theme",
		"macvim",
		"vim",
		"classic"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/juanvqz/macvim-theme"
	},
	"icon": "icon.png",
	"contributes": {
		"themes": [
			{
				"label": "MacVim Classic Dark",
				"uiTheme": "vs-dark",
				"path": "./themes/macvim-dark-color-theme.json"
			},
			{
				"label": "MacVim Classic Light",
				"uiTheme": "vs",
				"path": "./themes/macvim-light-color-theme.json"
			}
		]
	},
	"scripts": {
		"package": "vsce package",
		"publish": "vsce publish"
	}
}

